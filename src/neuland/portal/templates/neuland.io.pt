<!DOCTYPE html>
<html lang="${request.locale_name}">
	<head>
		<meta charset="utf-8">
		<title>NeulandIO</title>
		<link rel="stylesheet" href="${request.static_url('neuland.portal:static/vendor/bootstrap/css/bootstrap.css')}"/>
		<link rel="stylesheet" href="${request.static_url('neuland.portal:static/neuland/css/style.css')}"/>
		<script type="text/javascript">
			var templateUri = "${request.static_url('neuland.portal:static/neuland/template/')}";
		</script>
	</head>

	<body>
		<script type="text/x-handlebars">

            <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
              <div class="container">
                <div class="navbar-header">
                  <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                  </button>
                  {{#link-to 'index' class="navbar-brand"}}Neuland I.O.{{/link-to}}
                </div>
                <div class="collapse navbar-collapse">
                  <ul class="nav navbar-nav">
                    <li class="active">{{#link-to 'index'}}Home{{/link-to}}</li>
                    <li>{{#link-to 'register'}}Register{{/link-to}}</li>
                  </ul>
                </div><!--/.nav-collapse -->
              </div>
            </div>

            <div class="container">
              <div class="starter-template">
                {{outlet}}
              </div>
            </div>

		</script>
		<script type="text/javascript" src="${request.static_url('neuland.portal:static/vendor/jquery/jquery-2.1.1.js')}"></script>
		<script type="text/javascript" src="${request.static_url('neuland.portal:static/vendor/bootstrap/js/bootstrap.js')}"></script>
		<script type="text/javascript" src="${request.static_url('neuland.portal:static/vendor/handlebars/handlebars-v1.3.0.js')}"></script>
		<script type="text/javascript" src="${request.static_url('neuland.portal:static/vendor/ember/ember.prod.js')}"></script>
		<script type="text/javascript" src="${request.static_url('neuland.portal:static/neuland/template.js')}"></script>
        <script type="text/javascript" src="${request.static_url('neuland.portal:static/neuland/app.js')}"></script>
		<script type="text/javascript" src="${request.static_url('neuland.portal:static/neuland/router.js')}"></script>
 	</body>
</html>
